cps(140/120)

d1
$slow 2
$sometimesBy 0.3 (jux (rev))
$stack[
  s "tabla2(8, 14, {0, 3})" #gain "1.0" #n (irand 30),
  s "kon(8, 14, {0, 6})" #n (irand 50) #gain "1.2",
  s "bd" #gain "1.2" #shape "0.4",
  s "cp cp ~ cp ~ cp ~" #gain "1.0"
]

d2
