-- 00

hush

d1
  $ slow 2
  $s "aleksi"
  #cps(110/240)
  #n 1
  #gain "1.0"
  #lpf 600
  #speed "[1.0, 0.5]"
  |*|speed "[1.0, 1.01]"
  

d2
  $sometimes (rev)
  $jux (iter 8)
  $stack
  [
    s "gamdru(3, 16, 5)",
    s "gamsar(7, 16, 10)",
    s "gamket(3, 16)",
    s "gambon(5, 16)"
  ]
  #sustain "0.5"
  #gain "1.0"
  #n "{0 .. 100}%3"


d4
  $stack
  [
    s "uni:1(5, 16, 7)" #gain "0.9",
    s "uni(3, 16)"
  ]
  |*| gain "1.2"

d3
  -- $sometimes (rev)
  $jux ((5/16) <~)
  $s "kon(7, 16)"
  #gain "0.8"

  #n "{0 1 3 4}"

d5
  $slow 4
  $s "matsu"
  #gain "1.0"
  #speed "[1.0, 2.0]"

d1
  $s "bar(10, 16)"
  #gain "0.8"

d4
  $sound "jimsyn(11, 16)"
  #n "{20 1}"
  |*| gain "1.0"
  #lpf (range 2000 8000 $slow 12 $sine)
  #resonance "0.2"  

d3
  $jux ((5/16) ~>)
  $s "tet(5, 16)"
  #gain "1.1"
  #n 1

  #n "{0 1 2 3}%5"

-- 02

d4
  -- $every 8 (jux (rev))
  -- $sometimesBy 0.2 ((5/8) <~)
  $stack
  [
    s "distd*2", 
    s "bd cp bd hc" #n (irand 12),
    s "uni(6, 16, {0, 3})"
    #n "0 0 0 1 0 3 1 2"
  ]
  |*|gain "1.2"

d3
  $stack
  [
    s "super808(5, 16, 3)" #n "{-12 -7 -5 0 5 7}%3",
    s "ifdrums(6, 16, 0)"  #n "{1 2 3}%4",
    slow 2 $s "ifdrums(4, 16)" #gain "1.5"
  ]
  #shape "0.5"
  #gain "1.0"
--03

do 
  let chord1 = "g3'min" 
  d5
    $stack
    [
      sound "supersaw(9,16,10)" # note chord1 |+|note "12", 
      sound "supersaw(9,16,5)" # note chord1 |+|note "7",
      sound "supersaw(9,16,3)" # note chord1 |+|note "0"
    ]
    #sustain "0.12"
    #gain "0.7"
    #lpf (range 800 8000 $slow 16 $sine)
    #resonance "0.2"

d2
  $jux ((5/8) ~>)
  $s "deepsyn(5, 16, [0, 5])"
  #gain "1.0"
  #n "<6 7 8 9>*4"
  #up "[19, 24]"
  #speed "[1.01, 1.0]"
  #lpf (range 300 8000 $slow 8 $rand) #resonance "0.3"

--04

d3
  -- $sometimesBy 0.5 ((5/8) <~)
  $stack
  [
    -- s "tabla2(5, 16, {0})" #gain "1.3" #n (irand 30),
    s "kon(3, 16, {0})" #n "{0}%6" #gain "1.2",
    s "uni(6, 16)" #gain "1.5",
    s "uni uni ~ uni ~ uni ~ uni" #n 1 #gain "1.3"
  ]
  |*|gain "1.0"
  -- #hpf 4000

do
  let chord2 = "c'maj7"
  d5
    $jux ((5/8) ~>)
    $s "superhammond(5, 16, [0, 3])" #note chord2 |+|note "[0, 7]"
    #lpf (range 500 18000 $slow 16 $sine)
    #resonance "0.3"
    #voice 3
    #gain "0.7"
    |+|note "{0 -12 12 -7 7}%16"

d2
  $ sometimesBy 0.4 (rev)
  $ s "superbass*16"
  # sustain "0.1"
  # n (scale "indian" "{0 .. 7}%9")
  #lpf (range 100 1000 $rand)
  #resonance "0.3"
  #gain "0.8"

