d2
$ jux (iter 8)
$ stack[
   sound "cp(3,8,3)",
   sound "909*4 [~ 909:1]*2",
   sound "[~ hh] hc(2,8)",
   sound "bass(3, 8)"|+| vowel "u i e o w"
]

d1
$jux(iter 2)
$jux(iter 8)
$stack[
  -- sound "bass(5, 16)" #n "0 2 1 3 5 3 2 1",
  sound "glitch(7, 16, 9)" #n "0 2 1 3 5 3 2 1",
  sound "cp [cp cp*2] ~" #n "0 1 2 3 4 5",
  s "super808*2"
]

