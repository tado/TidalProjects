d1
$jux (iter 8)
$sometimesBy 0.4 (rev)
$stack[
 sound "hc(5, 8)",
 sound "ifdrums(5, 8, 3)",
 sound "~ hh bass bass"
]
#n "0 1 2 3 4 5"
