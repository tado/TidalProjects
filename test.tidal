cps(80/120)

hush

d1 $ sound "bd*2"

d2 $ sound "cp*8"

d3 $ sound "hc(7,32,10)"

d3 $ sound "hc(3,8,5)"

d4 $ sound "ifdrums(3,8,2)" #n "1 2 0 4 7"

hush

d2 $ sound "[~ ~ ~ cp]*2"

d2 $ sound "~ cp ~ ~ ~ cp ~ cp"

d1 $ sound "[~ bass]*2 [sn cp]"

